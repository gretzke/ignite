{
  "name": "@ignite/plugin-types",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./base/*": {
      "types": "./dist/base/*.d.ts",
      "default": "./dist/base/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "types": [
        "dist/types.d.ts"
      ],
      "base/*": [
        "dist/base/*.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc"
  },
  "devDependencies": {
    "typescript": "^5.9.2"
  }
}
